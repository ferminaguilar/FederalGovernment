{
  "name": "Drupal PHP 8.3 (Compose)",
  "dockerComposeFile": "docker-compose.yml",
  "service": "app",
  "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
  "containerEnv": {
    "DB_NAME": "drupal",
    "DB_USER": "drupal",
    "DB_PASS": "drupal",
    "DB_HOST": "db",
    "DB_PORT": "3306"
  },
  "postCreateCommand": "composer install"
}